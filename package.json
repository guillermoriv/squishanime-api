{
  "name": "squishanime-api",
  "version": "4.1.2",
  "description": "Squishanime is API to fetch all the data from the culture japonese know as anime.",
  "main": "./src/api/api.ts",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsnd --transpile-only --ignore-watch node_modules --respawn src/server.ts",
    "build": "tsc -p .",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "format": "prettier --check src/**/*.ts",
    "format:fix": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "squishanime",
    "squishanime api"
  ],
  "author": {
    "name": "Guillermo Rivas",
    "email": "guillermo.campanudo@gmail.com",
    "reason": "Full-Stack Developer",
    "social": {
      "github": "https://github.com/guillermoriv"
    }
  },
  "engines": {
    "node": ">= 16.x"
  },
  "bugs": {
    "url": "https://github.com/guillermoriv/squishanime-api/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/squishanime-api/squishanime.git"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.26.1",
    "body-parser": "^1.19.2",
    "cheerio": "^1.0.0-rc.10",
    "compose-middleware": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "mongodb": "^4.4.1",
    "mongoose": "^6.2.7"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/node": "^17.0.21",
    "@types/tough-cookie": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.0",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  }
